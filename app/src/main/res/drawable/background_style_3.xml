<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="@dimen/minos_small_padding"
        android:left="@dimen/small_padding"
        android:right="@dimen/small_padding"
        android:top="@dimen/small_padding">
        <shape android:shape="rectangle">

            <solid android:color="#00ffffff"></solid>

            <stroke
                android:width="1dp"
                android:color="#00b0c8fc"></stroke>

            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp"></padding>

            <corners android:radius="5dp"></corners>
        </shape>
    </item>
</layer-list>